int main() {
    // Write C code here
    int a,b;
    printf(" enter number a and b");
    scanf("%d%d",&a,&b);
    (a>b)?printf("larges value is a"):printf("largest value is b");

    return 0;
}
//9th program
// Online C compiler to run C program online
#include <stdio.h>
// year/400 ==0 leapyear
// year/100 = 0 not lleap year
// year/4==0 leap year
// not leap year
int main() {
    // Write C code here
  int year;
  printf("enter year");
  scanf("%d",&year);
  if(year%400==0){
      printf(" given year is leap year");
  }
else if(year%100==0){
      printf(" given year is not leap year");
}
else if(year%4==0){
      printf(" given year is leap year");
}
else{
          printf(" given year is not leap year");
}
    return 0;
}
