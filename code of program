// 1stprogram
// Online C compiler to run C program online
#include <stdio.h>
// a=1- b=20
int main() {
    int a,b;
    printf("eneter two number a and b\n");
    scanf("%d %d",&a,&b);
    if(a>b){
        printf(" a is the largestnumber");
    }
    else{
        printf("b is largest number");
    }
   

    return 0;
}

