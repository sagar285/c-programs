#include <stdio.h>
// a=1- b=20 20/2 ==0 7 14 21 28 35 28/7 ==0
int main() {
    int a,b;
    printf("eneter user number  \n");
    scanf("%d",&a);
      printf("eneter  specific number  \n");
    scanf("%d",&b);
    
    if(a%b==0){
        printf(" given number is multiple of specific number");
    }
    else{
        printf("given number is not multiple of specific number");
    }
   

    return 0;
}

//4th program
// Online C compiler to run C program online
#include <stdio.h>
// 2 1 3
// ascending order = 1 2 3
// descending order = 3 2 1
int main() {
    int a,b,c;
    printf("eneter  number a and b and c  \n");
    scanf("%d %d %d",&a,&b,&c);
    if((a>=b) && (a>=c)){
        if(b>=c){
            printf("ascending order=%d %d %d",c,b,a);
            printf("descending order=%d %d %d",a,b,c);
        }
        else{
             printf("ascending order=%d %d %d",b,c,a);
            printf("descending order=%d %d %d",a,c,b);
        }
        
    }
      else  if((b>=a) && (b>=c)){ // b a c
        if(a>=c){
            printf("ascending order =%d %d %d",c,a,b);
            printf("descending order =%d %d %d",b,a,c);
        }
        else{  // b c a
             printf("ascending order =%d %d %d",a,c,b);
            printf("descending order =%d %d %d",b,c,a);
        }
        
    }
      else  if((c>=b) && (c>=a)){
        if(a>=b){ // c a b
            printf("ascending order =%d %d %d",b,a,c);
            printf("descending order =%d %d %d",c,a,b);
        }
        else{  // c b a
             printf("ascending order =%d %d %d",a,b,c);
            printf("descending order =%d %d %d",c,b,a);
        }
        
    }
  
  
   

    return 0;
}

// 5th program

// Online C compiler to run C program online
#include <stdio.h>
#include<math.h>
// x=0 roots are equal
// x>0 roots are real and unequal
// x<0 roots areimaginary
int main() {
    
    double a,b,c,x,r1,r2,real,imag;
    printf("enter the coeffiecient fof a and b and c\n");
    scanf("%lf %lf %lf",&a,&b,&c);
    x=b*b-4*a*c;
    // condition for real and unequal roots
    if(x>0){
        r1=(-b+sqrt(x))/(2*a);
       r1=(-b-sqrt(x))/(2*a);
           printf(" r1 , r2 =%.2lf %.2lf",r1,r2);
    }
    else if(x==0){
        r1=r2 =-b/(2*a);
        printf(" r1 = r2 =%.2lf",r1);
    }
    else{
        real = -b/(2*a);
        imag=sqrt(-x)/(2*a);
        printf("r1=%.2lf + %.2lfi and r2 = %.2lf - %.2lfi",real,imag,real,imag);
    }
    

    return 0;
}

// 6th program 
// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
  int rollno,m1,m2,m3,total;
  float avg;
  printf("enter rollo no");
  scanf("%d",&rollno);
  printf(" enter marks of your subject");
  scanf("%d%d%d",&m1,&m2,&m3);
  total = m1+m2+m3;
  avg=total/3;
  if(avg>60){
      printf(" grade A");
  }
  else if((avg>=50) && (avg<60)){
      printf("Grade B");
  }
  else if((avg>=40) && (avg<50))
{
    printf("Grade c");
}
else {
    printf("fail");
}
    return 0;
}
//7th program

// Online C compiler to run C program online
#include <stdio.h>

int main() {
 int age;
 char ms,gender;
 printf(" enter marital status gender age\n");
 scanf("%c %c %d",&ms,&gender,&age);
 if(ms=='m'){
     printf("you can not marry bc you are already married");
 }
 else if(ms=='u'){
     if(gender=='m'){
         if(age>=21){
             printf(" you can marry");
         }
         else{
              printf(" you can not  marry");
         }
     }else if(gender=='f'){
         if(age>=18){
             printf(" you can marry"); 
         }
           else{
              printf(" you can not  marry");
         }
     }
     else{
         printf(" invalid input gender");
     }
 }
 else{
     printf(" invalid input marital status");
 }
   

    return 0;
}
